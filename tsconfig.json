{
  "compilerOptions": {
    "target": "es5",                                    
     "lib": ["es5", "dom"],                                      
    "module": "commonjs",                                
    "typeRoots": ["./node_modules/@types"],                                 
    "types": ["cypress","jest"],                                      
     "outDir": "./dist",                              
    "esModuleInterop": true,                           
    "forceConsistentCasingInFileNames": true,          
    "strict": true,                        
    "skipLibCheck": true                               
  },
  "include": [ "**/*.ts"],
  "exclude": ["node_modules"]
}
